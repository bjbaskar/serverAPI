		"inversify": "^5.0.1",
		"lodash": "^4.17.11",
		"lusca": "^1.6.1",
		"method-override": "^3.0.0",
		"mkdirp": "^0.5.1",
		"moment": "^2.24.0",
		"reflect-metadata": "^0.1.13",
		"request": "^2.88.0",
		"winston": "^3.2.1",
		"winston-daily-rotate-file": "^3.8.0"
	},
